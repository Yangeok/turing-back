{
  "name": "turing-back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "cross-env": "^5.2.0",
    "crypto": "^1.0.1",
    "dotenv": "^7.0.0",
    "faker": "^4.1.0",
    "forever": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-helmet": "^4.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "moment-timezone": "^0.5.23",
    "mysql2": "^1.6.5",
    "nodemailer": "^6.4.16",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport-facebook": "^3.0.0",
    "sequelize": "^5.8.8",
    "stripe": "^6.28.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "sequelize-cli": "^5.4.0"
  },
  "scripts": {
    "test:watch": "nodemon --exec cross-env NODE_ENV=test ./node_modules/.bin/mocha test/**/*.js -t 5000 -exit",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha test/**/*.js -t 5000 -exit && yarn coveralls",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "start": "cross-env NODE_ENV=production ./node_modules/.bin/forever src/app.js",
    "start:dev": "cross-env NODE_ENV=development nodemon src/app.js",
    "start:test": "cross-env NODE_ENV=test nodemon src/app.js",
    "db:generate": "sequelize model:generate",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate --env production",
    "db:migrate:undo": "sequelize db:migrate --env production",
    "db:migrate:test": "sequelize db:migrate --env test",
    "db:migrate:undo:test": "sequelize db:migrate:undo --env test",
    "db:seed": "sequelize db:seed:all --env production",
    "db:seed:undo": "sequelize db:seed:undo:all --env production",
    "db:seed:test": "sequelize db:seed:all --env test",
    "db:seed:undo:test": "sequelize db:seed:undo:all --env test",
    "migrate": "yarn db:migrate:undo && yarn db:migrate",
    "migrate:test": "yarn db:migrate:undo:test && yarn db:migrate:test",
    "seed": "yarn db:seed:undo && yarn db:seed",
    "seed:test": "yarn db:seed:undo:test && yarn db:seed:test"
  }
}
