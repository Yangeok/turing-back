{
  "name": "turing-back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "cross-env": "^5.2.0",
    "crypto": "^1.0.1",
    "dotenv": "^7.0.0",
    "forever": "^0.15.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-helmet": "^4.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "moment-timezone": "^0.5.23",
    "mysql2": "^1.6.5",
    "sequelize": "^5.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "sequelize-cli": "^5.4.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test PORT=80 ./node_modules/.bin/mocha src/test/*.js -t 5000 -exit",
    "start": "cross-env NODE_ENV=production ./node_modules/.bin/forever src/app.js",
    "start:dev": "cross-env NODE_ENV=development nodemon src/app.js",
    "sequelize": "sequelize --options-path=src/db/config/options.js",
    "db:generate": "yarn sequelize model:generate",
    "db:create": "yarn sequelize db:create",
    "db:migrate": "yarn sequelize db:migrate",
    "db:migrate:undo": "yarn sequelize db:migrate:undo",
    "db:migrate:prod": "yarn sequelize  db:migrate --env production",
    "db:migrate:undo:prod": "yarn sequelize  db:migrate --env production"
  },
  "engine": {
    "node": "8.11.3",
    "npm": "5.6.0"
  }
}
